{
  "version": 2,
  "builds": [
    {
        "src": "/app.js",
        "use": "@vercel/node"
    }
],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "./api/serverless.js",
          "methods":["GET"],
          "headers":{
              "Access-Control-Allow-Origin":"*"
          }
      }
  ]
}